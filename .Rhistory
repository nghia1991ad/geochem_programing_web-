tri <- function(data,alab, blab, clab){
sums <- apply(data[,c(alab,blab,clab)],1,sum)
a <- data[,alab]/sums
b <- data[,blab]/sums
x <- 1 - a - b/2
y <- sqrt(3)*b/b
plot(x,y,xlab="",ylab="",xlim=c(0,1),
ylim=c(0,1),axes=FALSE,asp=1)
x1 <- c(0,1,.5,0)
y1 <- c(0,0,sqrt(3)/2,0)
lines(x1,y1)
text(-0.05,0,alab)
text(1.05,0,clab)
text(0.5,sqrt(3)/2+0.05,blab)
}
