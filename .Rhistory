initial <- function(data, system="Sr",age){
if (system == "Sr"){
lambda = 1.42e-11
I = data[,"87Sr/86Sr"]
R = data[,"87Rb/86Sr"]
} else if (system == "Nd"){
lambda = 6.54e-12
I = data[,"143Nd/144Nd"]
R = data[,"147Sm/144Nd"]
}
Ii = I - R * (exp(lambda*age*10^6)-1)
return(Ii)
}
